<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net8.0;net9.0</TargetFrameworks>
    <PackageId>Seerstone.DateTimeNano</PackageId>
    <Version>1.1.0</Version>
    <Authors>Nich Overend</Authors>
    <Company>Seerstone</Company>
    <Description>A simple library to handle date and time in nanosecond precision.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/NichUK/DateTimeNano</RepositoryUrl>
    <PackageTags>csharp dotnet nuget DateTimeNano</PackageTags>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="protobuf-net.Core" Version="3.2.45" />
    <None Include="readme.md" Pack="true" PackagePath="readme.md" />
  </ItemGroup>
</Project>